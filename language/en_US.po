#: /var/www/phpav.net/module/ZfcUserAdmin/src/ZfcUserAdmin/Service/Mailer.php:58
#, php-format
msgid "zfcuseradmin.confirmation_mail.body_text%s%s"
msgstr ""
"Hi,\n"
"You have successfully registered on site %s, but your account is not "
"activated yet. \n"
"Please check your email and follow the activation link:\n"
"%s\n"
"\n"
"If you haven't been registering, please, ignore this message."

#: /var/www/phpav.net/module/ZfcUserAdmin/src/ZfcUserAdmin/Service/Mailer.php:62
#, php-format
msgid "zfcuseradmin.confirmation_mail.body_html%s%s"
msgstr ""
"<p>Hi</p>\n"
"<p>You have registered on site <strong>%1$s</strong>. To complete "
"registration and activate your account, please,  follow this link: </p>\n"
"<table class=\"button\" width=\"100%%\" border=\"0\" cellspacing=\"0\" "
"cellpadding=\"0\">\n"
"    <tr>\n"
"        <td>\n"
"            <table class=\"inner-wrapper\" border=\"0\" cellspacing=\"0\"  "
"cellpadding=\"0\" align=\"center\">\n"
"                <tr>\n"
"                    <td align=\"center\">\n"
"                        <a href=\"%2$s\" target=\"_blank\"> %2$s</a>\n"
"                    </td>\n"
"                </tr>\n"
"            </table>\n"
"        </td>\n"
"    </tr>\n"
"</table>\n"
"\n"
"<p>If you haven't been registering, please, ignore this message.</p>"

